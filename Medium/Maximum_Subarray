class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int cur = 0, prefix = INT_MAX, ans = INT_MIN;
        for(int i = 0; i < nums.size(); ++i){
            cur+=nums[i];
            ans = i ? max(ans,max(cur,cur-prefix)) : max(ans,cur);
            prefix = min(prefix,cur);
        }
        return ans;
    }
};